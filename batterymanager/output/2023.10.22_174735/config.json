{
  "type": "web",
  "devices": {
    "Pixel6": {}
  },
  "repetitions": 10,
  "duration": 100000,
  "browsers": ["chrome"],
  "paths": [
    "https://www.qq.com",
    "https://www.nytimes.com",
    "https://www.azure.com/",

  ],
  "usb_handler" : {
                "enable_command"  : "uhubctl -l 1-1  -a 1",
                "disable_command" : "uhubctl -l 1-1  -a 0"
                },
  "profilers": {
    "batterymanager": {
      "experiment_aggregation": "default",
      "sample_interval": 100,
      "data_points": [
        "BATTERY_PROPERTY_CURRENT_NOW", "EXTRA_VOLTAGE", "EXTRA_TEMPERATURE"
      ],
      "persistency_strategy": [
        "adb_log"
      ]
    }
  },
  "run_stopping_condition" : {"function" : {}},
  "scripts": {
    "before_experiment": "Scripts/before_experiment.py",
    "before_run": "Scripts/before_run.py",
    "after_launch": "Scripts/after_launch.py",
    "interaction": [
      {
        "type": "python3",
        "path": "Scripts/interaction3.py",
        "timeout": 100000,
        "logcat_regex": "<expr>"
      }
    ],
    "before_close": "Scripts/before_close.py",
    "after_run": "Scripts/after_run.py",
    "after_experiment": "Scripts/after_experiment.py"
  },
  "time_between_run": 1000
}
